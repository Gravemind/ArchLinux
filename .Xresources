!
! xrdb -merge -I$HOME ~/.Xresources
!

*background:  #202020
*foreground:  #BBBBBB

!black
*color0:      #000000
*color8:      #666666

!red
*color1:      #9e1828
*color9:      #cf6171

!green
*color2:      #aece92
*color10:     #B8EB6C
! c5f779

!yellow
*color3:      #968a38
*color11:     #fff796

!blue
*color4:      #557DA1
*color12:     #4B84B8

!magenta
*color5:      #963c59
*color13:     #cf9ebe

!cyan
*color6:      #45A397
*color14:     #71bebe

!white
*color7:      #bebebe
*color15:     #ffffff

URxvt*cursorColor:      yellow

URxvt*urgentOnBell:     true

URxvt*loginShell:       true
URxvt*saveLines:        100000
URxvt*buffered:         true
URxvt*font:             xft:DejaVu Sans Mono:size=11
URxvt*scrollBar_right:  false
URxvt*scrollBar:        false

URxvt*inheritPixmap: true
URxvt*transparent: true
URxvt*shading: 25

URxvt*scrollTtyOutput:      false
URxvt*scrollWithBuffer:     true
URxvt*scrollTtyKeypress:    true

!URxvt.perl-ext-common: default,selection,clipboard
URxvt*cutchars: "\\`\"'&()*,;<=>?@[]^{|}:/!"
!URxvt.selection.pattern-0: \][ ](.+)
!URxvt.selection.pattern-1: >>>[ ]+(.+)

! Clickable URL
URxvt.perl-ext-common:  default,matcher,vtwheel,selection-to-clipboard
URxvt.url-launcher:     /usr/bin/firefox
URxvt.matcher.button:   2
