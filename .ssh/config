Host *
    Protocol 2
    ForwardAgent no
    ForwardX11 no
    #PasswordAuthentication no

    # Force use IdentityFile
    IdentitiesOnly yes
    # Auto-add keys to ssh agent
    AddKeysToAgent yes

    #AddressFamily inet

Include ~/.ssh/config.local

